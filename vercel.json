{
  "name": "shade-route-app",
  "version": 2,
  "builds": [
    {
      "src": "frontend-pwa/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend-pwa/$1"
    }
  ],
  "buildCommand": "cd frontend-pwa && npm run build",
  "outputDirectory": "frontend-pwa/dist",
  "installCommand": "cd frontend-pwa && npm install"
}